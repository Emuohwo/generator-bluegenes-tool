{
	"env": {
		"browser": true,
    "es6": true,
    "jquery": true
	},
	"extends": [
    "eslint:recommended",
    "prettier"
  ],
  "plugins": [
    "html",
    "prettier"
  ],
  "parserOptions": {
    "sourceType": "module"
  },
	"rules": {
		"indent": [
			"error",
			"tab"
		],
		"linebreak-style": [
			"error",
			"unix"
		],
		"quotes": [
			"error",
			"single"
		],
		"semi": [
			"error",
			"always"
    ],
    "prettier/prettier": "error"
	}
}